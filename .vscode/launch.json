{
    "version": "0.2.0",
    "configurations": [
         
        {
            "name": "Python:Odoo12",
            "type":"python",
            "request":"launch",
            "stopOnEntry": false,
            "python":"${command:python.interpreterPath}",
            "console":"integratedTerminal",
            "program":"/opt/odoo/odoo/odoo-bin",
            "args": [
               "--config=/opt/odoo/odoo/odoo.conf",
            ],
            "cwd":"${workspaceRoot}",
            "env": {},
            "envFile":"/opt/odoo/odoo/.env",
            "debugOptions": [
                "RedirectOutput"
            ]
        }
    ]
}